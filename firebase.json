{
  "functions": [
    {
      "codebase": "default",
      "source": "functions"
    },
    {
      "codebase": "stripe",
      "source": "stripe"
    }
  ],
  "firestore": {
    "rules": "firestore.rules"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "site": "fresh25"
  }
}
